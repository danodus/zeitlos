BLOCK RESETPATHS;
BLOCK ASYNCPATHS;
## ULX3S v3.1.x

# The clock "usb" and "gpdi" sheet
LOCATE COMP "CLK_25" SITE "G2";
IOBUF  PORT "CLK_25" PULLMODE=NONE IO_TYPE=LVCMOS33;
FREQUENCY PORT "CLK_25" 25 MHZ;

# JTAG and SPI FLASH voltage 3.3V and options to boot from SPI flash
# write to FLASH possible any time from JTAG:
SYSCONFIG CONFIG_IOVOLTAGE=3.3 COMPRESS_CONFIG=ON MCCLK_FREQ=62 SLAVE_SPI_PORT=DISABLE MASTER_SPI_PORT=ENABLE SLAVE_PARALLEL_PORT=DISABLE;
# write to FLASH possible from user bitstream:
# SYSCONFIG CONFIG_IOVOLTAGE=3.3 COMPRESS_CONFIG=ON MCCLK_FREQ=62 SLAVE_SPI_PORT=DISABLE MASTER_SPI_PORT=DISABLE SLAVE_PARALLEL_PORT=DISABLE;

## USBSERIAL FTDI-FPGA serial port "usb" sheet
LOCATE COMP "UART0_TX" SITE "L4"; # FPGA transmits to ftdi
LOCATE COMP "UART0_RX" SITE "M1"; # FPGA receives from ftdi
IOBUF  PORT "UART0_TX" PULLMODE=UP IO_TYPE=LVCMOS33 DRIVE=4;
IOBUF  PORT "UART0_RX" PULLMODE=UP IO_TYPE=LVCMOS33;

## LED indicators "blinkey" and "gpio" sheet
LOCATE COMP "LED_B" SITE "C1";
LOCATE COMP "LED_G" SITE "C2";
LOCATE COMP "LED_R" SITE "B2";
IOBUF  PORT "LED_G" PULLMODE=NONE IO_TYPE=LVCMOS33 DRIVE=4;
IOBUF  PORT "LED_B" PULLMODE=NONE IO_TYPE=LVCMOS33 DRIVE=4;
IOBUF  PORT "LED_R" PULLMODE=NONE IO_TYPE=LVCMOS33 DRIVE=4;

